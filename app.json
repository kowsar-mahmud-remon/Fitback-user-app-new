{
  "expo": {
    "name": "FitBack",
    "slug": "fitback-user-android-app",
    "owner": "kowsar.remon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logo/icon.png",
    "userInterfaceStyle": "light",

    "newArchEnabled": true,

    "splash": {
      "image": "./logo/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": ["ios", "android"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.qitca.fitback",
      "googleServicesFile": "./google-services-user.json",
      "useNextNotificationsApi": true,
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./logo/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./logo/favicon.png"
    },
    "plugins": ["expo-notifications"],
    "extra": {
      "eas": {
        "projectId": "9febfa31-c651-40f0-927c-ad1227d7e9f5"
      }
    }
  }
}
